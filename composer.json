{
	"name": "alquemie/cdp-analytics",
	"description": "WordPress implementation of Segment analytics.js with support for external link tracking.",
	"type": "wordpress-plugin",
	"license": "GPL-3.0-or-later",
	"homepage": "https://alquemie.net/",
	"support": {
		"issues": "https://github.com/alquemie/cdp-analytics/issues",
		"source": "https://github.com/alquemie/cdp-analytics"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"exclude-from-classmap": [
			"/tests/"
		]
	},
	"require": {
		"php": "^7.4|^8",
		"composer/installers": "^2.0",
		"segmentio/analytics-php": "^3.5",
		"woocommerce/action-scheduler": "^3.5",
		"yahnis-elsts/plugin-update-checker": "^5.0",
		"qriouslad/codestar-framework": "v2.2.8.1"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master"
	},
	"config": {
		"sort-order": true,
		"vendor-dir": "lib",
		"allow-plugins": {
			"composer/installers": true
		}
	},
	"extra": {
		"installer-paths": {
				"lib/plugins/{$name}/": [
						"type:wordpress-plugin",
						"type:wordpress-muplugin"
				]
		}
},
	"scripts": {
		
	}
}
